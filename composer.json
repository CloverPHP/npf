{
    "name": "php-nf/npf",
    "description": "New PHP Framework",
    "version": "1.3.9",
    "keywords": ["php", "framework", "library"],
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "YK Low",
            "email": "lykillerl@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^5.6 || >=7.0",
        "paragonie/random_compat": "2.0",
        "khanamiryan/qrcode-detector-decoder": "^1.0",
        "endroid/qr-code": "^2.5",
        "geoip2/geoip2": "~2.0",
        "twig/twig": "~1.0",
        "aptoma/twig-markdown": "^2.0",
        "michelf/php-markdown": "~1.3",
        "ext-json": "*",
        "ext-simplexml": "*",
        "ext-zlib": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-mysqli": "*",
        "ext-bcmath": "*",
        "ext-gd": "*",
        "ext-exif": "*",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-fileinfo": "*",
        "ext-libxml": "*",
        "react/react": "1.0",
        "react/http": "^0.8.4",
        "react/mysql": "^0.5.4",
        "clue/redis-react": "^2.3",
        "mobiledetect/mobiledetectlib": "^2.8"
    },
    "scripts": {
        "post-package-install": [
            "Npf\\Build\\Setup::Install"
        ]
    },
    "autoload": {
        "psr-4": {
            "Npf\\": "src/",
            "Npf\\Build\\": "build/"
        }
    }
}